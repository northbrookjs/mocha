{
  "name": "@northbrook/mocha",
  "description": "Mocha Configuration for Northbrook",
  "version": "1.0.0",
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "bugs": {
    "url": "https://github.com/northbrookjs/northbrookjs/issues"
  },
  "config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": "@northbrook/commit-types"
    }
  },
  "dependencies": {
    "app-module-path": "^1.1.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0"
  },
  "devDependencies": {
    "buba": "^2.0.4",
    "ghooks": "^1.3.2",
    "northbrook": "^3.1.0",
    "validate-commit-msg": "^2.8.0"
  },
  "peerDependencies": {
    "northbrook": "^3.1.0"
  },
  "homepage": "https://github.com/northbrookjs/northbrookjs#readme",
  "keywords": [
    "mocha",
    "northbrook",
    "plugin",
    "testing"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northbrookjs/northbrookjs.git"
  },
  "scripts": {
    "commit": "northbrook commit",
    "postinstall": "node ./post-install.js",
    "release": "northbrook release",
    "test": "node script.js --coverage"
  }
}
